# This Menu created with YUMI https://www.pendrivelinux.com
default 1
timeout 30
color NORMAL HIGHLIGHT HELPTEXT HEADING
foreground=FFFFFF
background=000000
title --- Windows Installers ---
root
#start Win10Pro
title Restore Win10Pro - wimboot
map (hd0) (hd1)
map (hd1) (hd0)
map --hook
kernel (hd1,0)/multiboot/wimboot
kernel (hd1,0)/multiboot/wimboot
initrd @bootmgr=(hd1,0)/multiboot/packages/Win10Pro/bootmgr @bcd=(hd1,0)/multiboot/packages/Win10Pro/boot/bcd @boot.sdi=(hd1,0)/multiboot/packages/Win10Pro/boot/boot.sdi @boot.wim=(hd1,0)/multiboot/packages/Win10Pro/sources/boot.wim
#end Win10Pro
#start Win11Pro
title Install Win11Pro - wimboot
map (hd0) (hd1)
map (hd1) (hd0)
map --hook
kernel (hd1,0)/multiboot/wimboot
kernel (hd1,0)/multiboot/wimboot
initrd @bootmgr=(hd1,0)/multiboot/packages/Win11Pro/bootmgr @bcd=(hd1,0)/multiboot/packages/Win11Pro/boot/bcd @boot.sdi=(hd1,0)/multiboot/packages/Win11Pro/boot/boot.sdi @boot.wim=(hd1,0)/multiboot/packages/Win11Pro/sources/boot.wim
#end Win11Pro
title
root
#start WinXp64
title Boot Windows XP
map (hd2) (hd0)
map (hd0) (hd2)
root (hd2,0)
find --set-root /ntldr
chainloader /ntldr
title Reinstall Windows XP (Stage 1)
find --set-root /multiboot/ISOS/WinXp64.iso
map (hd0) (hd2)
map (hd2) (hd0)
map --mem /multiboot/ISOS/firadisk.img (fd0)
map --mem /multiboot/ISOS/firadisk.img (fd1)
map --mem /multiboot/ISOS/WinXp64.iso (0xff)
map --hook
chainloader (0xff)/I386/SETUPLDR.BIN
title Reinstall Windows XP (Stage 2)
find --set-root /multiboot/ISOS/WinXp64.iso
map (hd0) (hd2)
map (hd2) (hd0)
map --mem /multiboot/ISOS/WinXp64.iso (0xff)
map --hook
chainloader (hd0)+1
#end WinXp64
title
root
title <-- Back to Main Menu
root (hd0,0)
chainloader (hd0)+1
rootnoverify (hd0)
